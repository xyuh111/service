
.nav{
  box-sizing: border-box;
  padding:25rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
}
.nav image{
  width:120rpx;
  height:120rpx;
}
.nav .grays::after{
  content:"";
  display: block;
  width: 32rpx;
  height: 4rpx;
  background: #b5b5b5;
  border-radius: 6rpx;
}
.nav .text{
  line-height: 60rpx;
  height:60rpx;
  color:#479de6;
}
.nav .text::after{
  content:">>";
  color:#479de6;
}

